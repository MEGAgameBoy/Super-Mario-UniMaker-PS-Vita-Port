        ||(obj_uni_saveload_control.alarm[2] != -1)
            exit;
    }
    if(global.screenprotector == 1){
    
        if(!window_has_focus())
            audio_master_gain(0.3);
        else
            audio_master_gain(1);
    }
    else
        audio_master_gain(1);
};

