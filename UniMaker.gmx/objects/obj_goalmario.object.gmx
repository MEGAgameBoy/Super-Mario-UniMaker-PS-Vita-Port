        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Goal Mario logic

//Destroy all enemies
with obj_enemyparent
{
    with(instance_create(x,y,obj_smoke)) sprite_index = spr_statuesmoke
    instance_destroy()
}

//When in the physics or countdown state
if ready = 2
or ready = 3
{

    //Prevent physics parent from destroying (Hack)
    if bbox_top &gt; room_height
    {
        if ready = 2
            hspeed = -1
        y = room_height-32
        visible = 0
    }

///Default step script

//Wall collisions
event_user(3)

//Floor collisions
event_user(4)

    //If Mario reached a wall or fell out of the room when in the physics state
    if ready = 2
    and hspeed &lt; 0
    {

        //Countdown state
        ready = 3

        //Begin countdown state
        event_user(15)

        //Stop walking
        hspeed = 0
        image_speed = 0;
        image_index = 0;

    }

    //When on the ground, use walking sprite
    if gravity = 0{
        sprite_index = scr_mariowalk()
        image_speed = 0.25+abs(hspeed)/7.5; //&lt; hspeed/10
    };
    //Otherwise
    else
    {
        image_speed = 0;
        //Use the jump sprite
        sprite_index = scr_mariojump()

        //Use the correct image index
        if(vspeed &gt; 0)
            image_index = 1;
        else
            image_index = 0;

    }

}
</string>
